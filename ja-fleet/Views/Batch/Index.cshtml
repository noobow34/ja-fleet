@{
    Layout = "";
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title>バッチ起動画面</title>
    <link rel="stylesheet" href="/lib/bootstrap/css/bootstrap.min.css" />
    <link rel="stylesheet" href="/lib/jquery-ui/jquery-ui.min.css">
    <script src="https://ajax.aspnetcdn.com/ajax/jquery/jquery-3.4.1.min.js" integrity="sha384-vk5WoKIaW/vJyUAd9n/wmopsmNhiy+L2Z+SBxGYnUkunIxVxAv/UtMOhba/xskxh" crossorigin="anonymous"></script>
    <script src="https://ajax.aspnetcdn.com/ajax/bootstrap/3.3.7/bootstrap.min.js" crossorigin="anonymous" integrity="sha384-Tc5IQib027qvyjSMfHjOMaLkfuWVxZxUPnCJA7l2mCWNIpG9mGCD8wGNIcPD7Txa"></script>
    <script src="/lib/jquery-ui/jquery-ui.min.js"></script>
    <script tyep="text/javascript">
        $(function () {
            $('#wcLunch').click(function () {
                $.ajax({ url: '/Batch/RefreshWorkingStatusAndPhoto', type: "POST"})
                .done(function (data) {
                    $("#result").html(data);
                })
            });

            $('#photoAllLunch').click(function () {
                $.ajax({ url: '/Batch/RefreshPhoto', type: "POST" ,data : {mode:1}})
                .done(function (data) {
                    $("#result").html(data);
                })
            });

            $('#photoNotRetiredLunch').click(function () {
                $.ajax({ url: '/Batch/RefreshPhoto', type: "POST",data : {mode:2}})
                .done(function (data) {
                    $("#result").html(data);
                })
            });

            $('#photoRetiredLunch').click(function () {
                $.ajax({ url: '/Batch/RefreshPhoto', type: "POST",data : {mode:3}})
                .done(function (data) {
                    $("#result").html(data);
                })
            });

        });
    </script>
</head>
<body>
    <div class="container">
        <div class="input-group" style="width:200px;margin-top:15px;">
            <button id="wcLunch" type="button" class="btn">ステータス・写真更新通常ジョブ</button><br />
            <button id="photoAllLunch" type="button" class="btn">写真更新（全部）</button><br />
            <button id="photoNotRetiredLunch" type="button" class="btn">写真更新（退役以外）</button><br />
            <button id="photoRetiredLunch" type="button" class="btn">写真更新（退役）</button><br />
            <div id="result" style="color:red"></div>
        </div>
    </div>
</body>
</html>